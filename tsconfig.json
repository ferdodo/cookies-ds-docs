{
	"compilerOptions": {
		"target": "es6",
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitAny": false,
		"skipLibCheck": true,
		"strict": true,
		"noEmit": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"module": "commonjs",
		"lib": ["dom", "es2018"]
	}
}
